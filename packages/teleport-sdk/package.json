{
  "name": "teleport-sdk",
  "version": "1.0.0",
  "description": "Frontend SDK to @makerdao/dss-teleport",
  "files": [
    "/dist"
  ],
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --check \"./**/*.ts\"",
    "format:fix": "prettier --write \"./**/*.ts\"",
    "lint": "eslint --ext .ts src test scripts",
    "lint:fix": "yarn lint --fix",
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "test:fix": "yarn lint:fix && yarn format:fix && yarn test && yarn typecheck",
    "typecheck": "yarn eth-sdk && tsc --noEmit",
    "build": "yarn eth-sdk && rm -rf ./dist && tsc -p ./tsconfig.build.json && cp -R ./src/sdk ./dist/sdk",
    "demo:arbitrum": "yarn ts-node ./scripts/demo.arbitrum.ts | grep -v \"Duplicate definition of File\"",
    "demo:optimism": "yarn ts-node ./scripts/demo.optimism.ts | grep -v \"Duplicate definition of File\""
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@arbitrum/sdk": "^1.1.2",
    "axios": "^0.26.0",
    "ethers": "^5.5.4",
    "ts-essentials": "^9.1.2"
  },
  "devDependencies": {
    "@dethcrypto/eth-sdk": "0.3.1",
    "@ethereum-waffle/chai": "^4.0.0-alpha.0",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.0",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.11.0",
    "eslint-config-typestrict": "^1.0.2",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.12.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "mocha": "^9.2.1",
    "prettier": "^2.5.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5"
  }
}
