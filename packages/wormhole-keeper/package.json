{
  "name": "wormhole-keeper",
  "version": "1.0.0",
  "description": "Keeper for wormhole sdk",
  "files": [
    "/dist"
  ],
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node ./src",
    "format": "prettier --check \"./**/*.ts\"",
    "format:fix": "prettier --write \"./**/*.ts\"",
    "lint": "eslint --ext .ts src",
    "lint:fix": "yarn lint --fix",
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "test:fix": "yarn lint:fix && yarn format:fix && yarn test && yarn typecheck",
    "typecheck": "tsc --noEmit",
    "build": "rm -rf ./dist && tsc -p ./tsconfig.build.json && cp -R ./src/sdk ./dist/sdk"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@eth-optimism/sdk": "^1.1.0",
    "@types/node": "^17.0.23",
    "axios": "^0.26.0",
    "ethers": "^5.5.4",
    "ts-essentials": "^9.1.2"
  },
  "devDependencies": {
    "@arbitrum/sdk": "^1.1.2",
    "@ethereum-waffle/chai": "^4.0.0-alpha.0",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/lodash": "^4.14.181",
    "@types/mocha": "^9.1.0",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.11.0",
    "eslint-config-typestrict": "^1.0.2",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.12.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "lodash": "^4.17.21",
    "mocha": "^9.2.1",
    "prettier": "^2.5.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5"
  }
}
